SELECT * FROM DUAL WHERE DUMMY LIKE q'[%'%]';---FOR CHECKING WHETHER COLUMN HAS ' IN THE COLUMN DATA OR NOT
----
--BELOW IS SHOW CASING LOOPS
---
DECLARE 
  I NUMBER(1);
  J NUMBER(1);
BEGIN
  <<OUTER_LOOP>>
  FOR I IN 1..9 LOOP
    <<INNER_LOOP>>
    FOR J IN 1..9 LOOP
      DBMS_OUTPUT.PUT_LINE('I IS: ' ||I|| ' J IS: ' ||J)
    END LOOP INNER_LOOP;
  END LOOP OUTER_LOOP;
END;
/
